<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>cloud.ondra.dt-backup-all-databases</string>
		<key>ProgramArguments</key>
		<array>
			<string>/bin/zsh</string>
			<string>-c</string>
			<string>/Users/ondra/bin/dt-backup/dt-staged-backup-01-export-all.zsh</string>
		</array>
		<key>RunAtLoad</key>
		<true/>
		<key>StartInterval</key>
		<integer>600000</integer>
		<key>StandardErrorPath</key>
		<string>/tmp/dt-backup-all-databases.log</string>
		<key>StandardOutPath</key>
		<string>/tmp/dt-backup-all-databases.log</string>
	</dict>
</plist>
